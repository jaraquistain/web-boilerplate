{
  "name": "web-boilerplate",
  "private": true,
  "version": "0.0.1",
  "description": "My personal web project boilerplate",
  "main": "./bin/entry.js",
  "scripts": {
    "server:prep": "universal-webpack --settings ./src/webpack/universal-webpack-settings.json prepare",
    "dev": "npm-run-all server:prep --parallel dev:*",
    "dev:wds": "webpack-dev-server --config './src/webpack/webpack.config.client.dev.babels.js' --colors",
    "dev:watch": "nodemon './bin' --watch './static/dist'",
    "dev:webpack": "webpack --progress --watch --config './src/webpack/webpack.config.server.dev.js' --colors --display-error-details"
  },
  "repository": {
    "type": "git",
    "url": "git+https://jaraquistain@github.com/jaraquistain/web-boilerplate.git"
  },
  "author": "Jon Araquistain (jon@araquista.in)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaraquistain/web-boilerplate/issues"
  },
  "homepage": "https://github.com/jaraquistain/web-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.1",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "universal-webpack": "^0.3.11",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "koa": "^2.3.0",
    "koa-favicon": "^2.0.0",
    "koa-helmet": "^3.2.0",
    "koa-static": "^4.0.1",
    "lodash.merge": "^4.6.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "serialize-javascript": "^1.3.0"
  }
}
